<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.projeto6">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/backpack"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/backpack"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AnalandiaActivity"
            android:label="@string/category_analandia" />

        <activity android:name=".ItirapinaActivity"
            android:label="@string/category_itirapina" />

        <activity android:name=".AguasDaPrataActivity"
            android:label="@string/category_aguasDaPrata" />

        <activity android:name=".BrotasActivity"
        android:label="@string/category_brotas" />

    </application>

</manifest>